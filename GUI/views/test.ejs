function isVariConsValid()
{
	if(isBlankOrNotNumberCtl(document.getElementById("txtNoOfVari"))
	|| isCtlValNotBetween(document.getElementById("txtNoOfVari"),1,50)
	)
		return false;

	if(isBlankOrNotNumberCtl(document.getElementById("txtNoOfCons"))
	|| isCtlValNotBetween(document.getElementById("txtNoOfCons"),1,50)
	)
		return false;

	return true;
}

function isInputValid(BConstraintDelete)
{
	if(typeof(BConstraintDelete) == "undefined") //because Extras passed in isNumber() and passed undefined if not passed by user
	    BConstraintDelete = false;

	if(!isVariConsValid())
		return false;

	var NoOfCons = eval(document.getElementById("txtNoOfCons").value);
	var NoOfVari = eval(document.getElementById("txtNoOfVari").value);
	var i,j;

    if(NoOfVari == 0)
    {
	    alert("Please enter variables");
	    document.getElementById("txtNoOfVari").focus();
		return false;
    }
    if(NoOfCons == 0)
    {
	    alert("Please enter constraints value");
	    document.getElementById("txtNoOfCons").focus();
		return false;
    }

	if(document.getElementById('ddlMinMax').selectedIndex==-1)
	{
	    LLPToTbl();
    }

	if(document.getElementById('ddlMinMax').selectedIndex==-1)
	{
	    alert("Please select either min or max")
	    document.getElementById('ddlMinMax').focus();
		return false;
    }

	var SMethod = document.getElementById("ContentPlaceHolder1_ddlFind").value;
	if(SMethod=="gm" && NoOfVari != 2)
	{
        alert("Please enter only 2 variables, for graphical method");
        document.getElementById("txtNoOfVari").focus();
	    return false;
	}

	//-->Cj VALIDATION
	//var SCj = getCj();//"-15,-10";
	for(j=1;j<=NoOfVari;j++)
	{
		if(isBlank(document.getElementById('txtCj'+j).value))
		    document.getElementById('txtCj'+j).value="0";
		else if(document.getElementById('txtCj'+j).value=='-')
		    document.getElementById('txtCj'+j).value="-1";

		//S += document.getElementById('txtCj'+j+'').value;
		if(isBlankOrNotNumberCtl(document.getElementById('txtCj'+j)))
			return false;

	}
	//<--Cj VALIDATION


	//-->XSij VALIDATION
	//var SXSij = getXSij();//"-3,-5;-5,-2";
	for(i=1;i<=NoOfCons;i++)
	{
	    var ZeroCnt=0;
		for(j=1;j<=NoOfVari;j++)
		{
			if(isBlank(document.getElementById("txtXSij_"+i+"_"+j+"").value))
			    document.getElementById("txtXSij_"+i+"_"+j+"").value="0";
			else if(document.getElementById("txtXSij_"+i+"_"+j+"").value=='-')
			    document.getElementById("txtXSij_"+i+"_"+j+"").value="-1";

			//S += document.getElementById('txtXSij'+i+''+j+'').value;
			if(isBlankOrNotNumberCtl(document.getElementById("txtXSij_"+i+"_"+j+"")))
				return false;

			if(eval(document.getElementById("txtXSij_"+i+"_"+j+"").value)==0)
			    ZeroCnt++;
		}

		//constraint with all coeff zeros
		if(BConstraintDelete && ZeroCnt==NoOfVari)
		{
		    //alert("Constraint: "+ i +", is blank, so either enter value or remove this");
		    var SMsg = "Constraint: "+ i +", is blank, So are you sure to remove constraint: "+ i +"";
		    if(i<NoOfCons)
		        SMsg = "Constraint: "+ i +", is blank, So are you sure to remove constraint: "+ i +" to constraint: "+ NoOfCons +"";

		    if(confirm(SMsg))
		    {
		        document.getElementById("txtNoOfCons").value = i-1;
		        BtnGenerate_onclick();
		        TblToLLP();
		    }
		    else
		    {
		        document.getElementById("txtXSij_"+i+"_"+1+"").focus();
		        document.getElementById("txtXSij_"+i+"_"+1+"").select();
		    }
		    return false;
		}
		//1

		if(document.getElementById("ddlXSign"+i).selectedIndex==-1)
	    {
	        alert("Please select sign")
	        document.getElementById("ddlXSign"+i).focus();
		    return false;
        }

	}
	//<--XSij VALIDATION

	//-->Xb VALIDATION
	//var SXb = getXb();//"-5,-3";
	for(i=1;i<=NoOfCons;i++)
	{
		if(isBlank(document.getElementById('txtXb'+i).value))
		    document.getElementById('txtXb'+i).value="0";
		else if(document.getElementById('txtXb'+i).value=='-')
		    document.getElementById('txtXb'+i).value="-1";

		//S += document.getElementById('txtXb'+i+'').value;
		if(isBlankOrNotNumberCtl(document.getElementById('txtXb'+i)))
			return false;
	}
	//<--Xb VALIDATION

	//-->XSign VALIDATION
	//var SXSign = getXSign();//"<=,<=";
	//<--XSign VALIDATION

	//-->MinMax VALIDATION
	//var SMinMax = getMinMax();//"Max";
	//<--MinMax VALIDATION

	if(document.getElementById('ContentPlaceHolder1_ddlMode').value=="")
	{
	    document.getElementById('ContentPlaceHolder1_ddlMode').value="D";
    }

    if(NoOfCons < 1)
    {
	    alert("Please enter more than 1 constraints");
	    document.getElementById("txtNoOfCons").focus();
		return false;
    }

	return true;
}

