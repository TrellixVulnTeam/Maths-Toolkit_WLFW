function getCj()
{
	var S = "";
	var NoOfCons = eval(document.getElementById("txtNoOfCons").value);
	var NoOfVari = eval(document.getElementById("txtNoOfVari").value);

	for(j=1;j<=NoOfVari;j++)
	{
		S += removeComma(document.getElementById('txtCj'+j+'').value);
		if(j<NoOfVari)
			S += ",";
	}

	return S;
}

function getXSij()
{
	var S = "";
	var NoOfCons = eval(document.getElementById("txtNoOfCons").value);
	var NoOfVari = eval(document.getElementById("txtNoOfVari").value);

	for(i=1;i<=NoOfCons;i++)
	{
		for(j=1;j<=NoOfVari;j++)
		{
			S += removeComma(document.getElementById("txtXSij_"+i+"_"+j+"").value);
			if(j<NoOfVari)
				S += ",";
		}
		if(i<NoOfCons)
			S += ";"
