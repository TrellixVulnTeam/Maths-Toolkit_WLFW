<script language="javascript">

String.prototype.trim = function() {
    return this.replace(/^\s+|\s+$/g, "");
};

function LLPToTbl()
{
    clearTextarea();

    //replace UnicodeChar eg '-'
	if(isUnicodeCharInCtl(document.getElementById("txtLPP")))
		return false;
    //isUnicodeCharInCtl(document.getElementById("txtLPP"));

	var QStr = document.getElementById("txtLPP").value;
	//alert(QStr);

	//replace some symbols
	QStr = QStr.replace(/≤/g,"<");
	QStr = QStr.replace(/≥/g,">");
	QStr = QStr.replace(/<=/g,"<");
	QStr = QStr.replace(/>=/g,">");
	//QStr = QStr.replace(/ /g,"");
	QStr = QStr.replace(/\(/g,"");
	QStr = QStr.replace(/\)/g,"");

    //replace ',' to new line
	QStr = QStr.replace(/\,/g,"\r\n");

    //remove tab
	QStr = QStr.replace(/\t/g,"");

    //remove ltrim
	QStr = QStr.replace(/ [ ]+/g,'');

	//replace double lines to single line
	QStr = QStr.replace(new RegExp('[\r\n]+','g'), '\r\n');

	//remove starting new line char
	QStr = QStr.replace(new RegExp('^[\r\n]+'), '');

    //if subject keyword is not in new line then add new line
    if(QStr.indexOf("\r\nsub")==-1)
    {
	    QStr = QStr.replace('sub', '\r\nsub');
    }

    //if and keyword is not in new line then add new line
    if(QStr.indexOf("\r\nand")==-1)
    {
	    QStr = QStr.replace('and', '\r\nand');
    }

	disp("11-->"+QStr);

	//convert string to array
    var QA = QStr.toString().split('\r\n');
    disp("22-->"+QA);
    //alert(QA.length);

    var BUnResStr = false;
    var SUnResStr="";
    //-->remove subject to the constraints words and blank lines
    var ISt;
    for(ISt = QA.length-1;ISt>=0;ISt--)//for(ISt = 1;ISt<QA.length;ISt++)
    {
        var SSubjTo = QA[ISt].toLowerCase().trim();
        //disp("SSubjTo:"+ ISt +" = "+SSubjTo.length);
        if(SSubjTo.indexOf("sub")!=-1 || SSubjTo.indexOf("to")!=-1 || SSubjTo.indexOf("the")!=-1 || SSubjTo.indexOf("cons")!=-1 || SSubjTo.length==0)
        {
            QA.splice(ISt, 1);
            //break;
        }
        else if(SSubjTo.indexOf("and")==-1 && isNumberExistsAtAnyPos(SSubjTo)==false)
        {
            //in whole line only chararter exist without any number then remove
            QA.splice(ISt, 1);
        }
        else if(SSubjTo.indexOf("unres")!=-1 || (ISt==QA.length-1 && SSubjTo.indexOf("integer") != -1))
        {
            //only unrestricted value and delete current line
            SUnResStr = QA[ISt];
            QA.splice(ISt, 1);
            BUnResStr = true;
        }
        else if(SSubjTo.indexOf("and")!=-1)
        {
            //remove all element 'and' and its below
            for(var i=QA.length-1;i>=ISt;i--)
            {
                //for all element 'and' and its below in SUnResStr and delete
                SUnResStr = QA[i] +","+ SUnResStr;
                QA.splice(i, 1);
            }
        }
    }
    disp("SUnResStr-->"+SUnResStr);
    disp("33-->"+QA);
    //<--remove subject to the constraints words and blank lines

    //-->unrestricted
    var SVarUnRes="";
    if(BUnResStr)//if(SUnResStr!="")
    {
        //remove space from left and right side of '>' symbol
        SUnResStr = SUnResStr.replace(/[ ]+>[ ]+/g,"\>");

        //replace all 'and' with ',' sign
        SUnResStr = SUnResStr.replace(/and/g,",");

        var ind = SUnResStr.indexOf(">0");
        if(ind!=-1)
            SUnResStr = SUnResStr.substring(ind+3).trim();
        disp("2->SUnResStr-->"+SUnResStr);

//        ind = SUnResStr.indexOf("unres");
//        SUnResStr = SUnResStr.substring(0,ind).trim();
//        disp("1->SUnResStr-->"+SUnResStr);


        //-->now split and join
        var UnResA = SUnResStr.split(',');
        for(var i=0;i<UnResA.length;i++)
        {
            UnResA[i]=UnResA[i].trim();
            if(i==UnResA.length - 1)
                UnResA[i] = UnResA[i].substring(0, UnResA[i].indexOf(' '));

            if(UnResA[i]!="")
            {
                if(SVarUnRes!="")
                    SVarUnRes += ",";

                SVarUnRes += UnResA[i];
            }
        }
        //<--now split and join
        disp("3->SVarUnRes:"+ SVarUnRes + "");
    }
    //<--unrestricted

    //-->objective function
    //-->1.Find Max or Min function
    var SMinMax = "";
    var ObjFunc = QA[0];
    var SInd = ObjFunc.toLowerCase().indexOf("max"); SMinMax = "Max";
    if(SInd==-1)
    {
        SInd = ObjFunc.toLowerCase().indexOf("min"); SMinMax = "Min";
        if(SInd==-1)
        {
            SMinMax = "";
            alert("Error: Please enter Max or Min in objective function");
            return;
        }
    }
    var EInd = ObjFunc.indexOf(" ");
    ObjFunc = ObjFunc.substring(EInd+1).trim();
    disp("ObjFunc:" + ObjFunc);
    //<--1.Find Max or Min function

    //-->2. find Z name
    SInd = ObjFunc.indexOf("=");
    if(SInd==-1)
    {
        alert("Error: Please enter '=' in objective function");
        return;
    }
    var SZ = ObjFunc.substring(0,SInd).trim();
    ObjFunc = ObjFunc.substring(SInd+1).trim();
    disp("SZ:" + SZ);
    disp("ObjFunc:" + ObjFunc);
    //<--2. find Z name

    //-->3. find obj func variables and coefficient
    var SAns = getVariableAndCoefficient(ObjFunc);
    disp("SAns:" + SAns.join(";"));
    var SVar = SAns[0];
    var SCj = SAns[1];
    var Nv = SAns[0].length;
//    disp("SAns.length=" + SAns.length);
//    disp("SAns[0].length=" + SAns[0].length);
    //<--3. find obj func variables and coefficient
    //<--objective function

    //-->constraints
    var Sign = ["<",">","="];
    var SXSign = [],SXb=[];
    var k=0;
    var SXSij = [];
    for(var i = 1; i<QA.length; i++,k++)
    {
        var Constraint = QA[i];

        //-->Find sign and Xb
        for(var j=0;j<Sign.length;j++)
        {
            SInd = Constraint.indexOf(Sign[j]);
            if(SInd!=-1)
            {
                SXSign[k] = Sign[j];
                SXb[k] = Constraint.substring(SInd+1).trim();
                Constraint = Constraint.substring(0,SInd).trim();
                break;
            }
        }
        SAns = getVariableAndCoefficient(Constraint);
        SXSij[k] = arrangeCoeff(SVar,SAns);
        disp("SAns=" + SAns.join(";"));
//    disp("SAns.length=" + SAns.length);
//    disp("SAns[0].length=" + SAns[0].length);
    }
    var Nc = k;
    SXSij = SXSij.join(";")
    //<--constraints


//		Nv=3;
//		Nc=3;
//		SCj = "3,5,4";
//		SXSij = "2,3,0;0,2,5;3,2,4";
//		SXb = "8,10,15";
//		SXSign = "<=,<=,<=";
//		SMinMax = "max";
//      SZ = "Z";
//      SVar = "x1,x2,x3";
//		SVarUnRes = "x1";

    //final sol display
    disp("--------------------------------------");
    disp("Nv:"+ Nv + "; Nc:"+Nc);
    disp("SMinMax:"+SMinMax + "; SZ:"+ SZ + "; SVar:"+ SVar + "; SCj:"+SCj);
    disp("SXSij:"+SXSij + "  SXSign:"+ SXSign + "; SXb:"+SXb);
    disp("SVarUnRes:"+ SVarUnRes + "");
    disp("--------------------------------------");

	//to disp single lpp problem
    //setLLP(Nv, Nc, SMinMax, SZ, SVar, SCj, SXSij, SXSign, SXb, SVarUnRes);

//	//to disp in table format
	setContentS(Nv, Nc, SMinMax, SZ, SVar, SCj, SXSij, SXSign, SXb, SVarUnRes);
}

function arrangeCoeff(SVar,SAns)
{
    var SCoeff = [];
    for(var k=0;k<SVar.length;k++)
    {
        SCoeff[k] = 0;
    }

    for(var j=0;j<SAns[0].length;j++)
    {
        for(var k=0;k<SVar.length;k++)
        {
            if(SAns[0][j]==SVar[k])
            {
               SCoeff[k] = SAns[1][j];
               break;
            }
        }
    }

    return SCoeff;
}

function getVariableAndCoefficient(SPoly)
{
    SPoly = SPoly.replace(/ /g,"");
    //disp(SPoly)

    //1st char is either + or - sign
    if (SPoly.charAt(0) != "+" && SPoly.charAt(0) != "-")
        SPoly = "+"+ SPoly;

	SPoly = SPoly.replace(/\+/g,"~+");
	SPoly = SPoly.replace(/\-/g,"~-");
    //disp(SPoly)

	//convert string to array
    var SPolyA = SPoly.toString().split("~");
    //disp(SPolyA);

    var SVar = [];
    var SCoeff = [];

    for (var i = 1; i < SPolyA.length; i++)
    {
    	SVar[i-1] = /([a-zA-Z].*)/.exec(SPolyA[i])[1];
    	SCoeff[i-1] = SPolyA[i].replace(/[a-zA-Z].*/, '');

        if(SCoeff[i-1] == "+")
            SCoeff[i-1] = "1";
        else if(SCoeff[i-1] == "-")
            SCoeff[i-1] = "-1";

    	SCoeff[i-1] = SCoeff[i-1].replace(/\+/g,"");
    }
    //disp("SVar:"+SVar);
    //disp("SCoeff:"+SCoeff);

    var SAns = [SVar, SCoeff];
    return SAns;
}

function isNumberExistsAtAnyPos(S)
{
	var number = "+-0123456789";

	for(var i=0;i<S.length;i++)
	{
		if(number.indexOf(S.substring(i,i+1)) != -1)
			return true;
	}

	return false;
}

function disp(x)
{
    document.getElementById("txtA").value += x + "\r\n\r\n";
}

function clearTextarea()
{
    document.getElementById("txtA").value="";
}

</script>
