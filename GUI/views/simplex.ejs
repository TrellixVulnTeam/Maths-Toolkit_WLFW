                BFlg=true;
                break;
            }
        }
        if(BFlg==false)
        {
            if(S1!="")
                S1 += ",";
            S1 += SVars[i];
        }
        else
        {
            if(S2!="")
                S2 += ",";
            S2 += SVars[i];
        }
    }
    SLPP += "\n";

    var SUnResOrIntMsg = "unrestricted in sign";
    if(document.getElementById("ContentPlaceHolder1_hMethod").value=="is")
        SUnResOrIntMsg = "non-negative integers";

    if(S1!="")
        SLPP += "and "+S1+ " >= 0 "

    if(S2!="")
        SLPP += "and "+S2+" "+SUnResOrIntMsg;
   //<--Variables and Unrestricted variable message

    disp(SLPP);
    document.getElementById("txtLPP").value = SLPP;
}
